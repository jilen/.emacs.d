{
	"name": "ensime",
	"publisher": "ENSIME",
	"displayName": "ENSIME Language Server",
	"description": "Scala language support via ensime",
	"license": "GPL-3.0+",
	"version": "0.0.1",
	"repository": {
		"url": "https://ensime.org"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:scala"
	],
	"keywords": [
		"ensime",
		"scala",
		"language"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
                "package": "vsce package"
	},
	"extensionDependencies": [
		"scala-lang.scala"
	],
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"vsce": "^2.11.0",
		"vscode-languageclient": "^6.1.3"
	}
}
